{% extends "AdminUserBundle::layout.html.twig" %}

{% block title "Social Maps - Email Send" %}

{% block content %}

<a href="{{ path('adminuser_userlist_paginated', {'page': 1}) }}"><< Back To User List</a>
<table class="table table-striped table-bordered table-condensed">

    <tr>
        <td>
            <form action="" method="POST" id="update_form">
                <h2 class="form-signin-heading">Send Email To User</h2>
                {{ form_errors(form) }}

                {{ form_rest(form) }}
                <input type="submit" value="Send Email" class="btn btn-primary"/>
                <button  onclick="history.go(-1)" class="btn btn-info" type="button" id="commonCancel" name="Cancel">Cancel</button>
            </form>
        </td>
    </tr>
</table>
{% endblock %}
